{
  "name": "lamool",
  "version": "0.0.4",
  "description": "Universal AWS Lambda runtime",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf src/**/*.js src/**/*.js.map",
    "watch": "tsc --watch",
    "test": "jest --verbose false --detectOpenHandles --collectCoverageFrom=!__tests__/*.ts",
    "test:browser": "webpack-dev-server --config webpack.sandbox.config.js",
    "codecov": "npm run test && codecov",
    "doc": "typedoc --out ./docs -m \"umd\"",
    "lint": "npm run pretty-quick && tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' --fix",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run clean && npm run build"
  },
  "files": [
    "src/*.js",
    "src/*.d.ts"
  ],
  "types": "src/index.d.ts",
  "keywords": [
    "amazon",
    "aws",
    "lambda",
    "universal",
    "isomorphic",
    "web worker",
    "worker"
  ],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/jszip": "^3.1.4",
    "@types/node": "^10.12.18",
    "@types/workerpool": "^2.3.0",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "aws-sdk": "^2.387.0",
    "axios": "^0.18.0",
    "jszip": "^3.1.5",
    "workerpool": "^3.0.0"
  }
}
