{
  "name": "lamool",
  "version": "0.0.3",
  "description": "Universal AWS Lambda runtime",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf src/**/*.js src/**/*.js.map",
    "watch": "tsc --watch",
    "test": "jest --verbose false --detectOpenHandles --collectCoverageFrom=!__tests__/*.ts",
    "test:browser": "webpack-dev-server --config webpack.sandbox.config.js",
    "codecov": "npm run test && codecov",
    "doc": "typedoc --out ./docs -m \"umd\"",
    "lint": "npm run pretty-quick && tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' --fix",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run clean && npm run build"
  },
  "files": [
    "src/*.js",
    "src/*.d.ts"
  ],
  "types": "src/index.d.ts",
  "keywords": [
    "amazon",
    "aws",
    "lambda",
    "universal",
    "isomorphic",
    "web worker",
    "worker"
  ],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/jszip": "^3.1.4",
    "@types/node": "^10.0.3",
    "@types/workerpool": "^2.3.0",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.11.0",
    "typescript": "^3.0.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "aws-sdk": "^2.344.0",
    "jszip": "^3.1.5",
    "workerpool": "^2.3.3"
  }
}
